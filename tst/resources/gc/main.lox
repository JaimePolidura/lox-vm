struct Child {
    name;
}

struct Parent {
    child;
}

fun createHeap(willStartGc) {
    for(var i = 0; i < 1000; i = i + 1) {
        var structObject = Parent{Child{selfThreadId() + " " + i}};
    }

    if(willStartGc) {
        sleep(100);
        forceGC();
    }
}

parallel createHeap(false);
parallel createHeap(false);
parallel createHeap(false);
parallel createHeap(true);

join();